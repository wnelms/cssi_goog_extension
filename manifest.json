{
  "manifest_version": 2,
  "name": "wow.ext",
  "version": "0.2",
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action":  {
    "default_icon": "volume.png",
    "default_popup": "popup.html",
    "default_title": "change your volumes"
  },
  "permissions": [
    "tabs"
  ]
}